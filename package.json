  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "alpinejs": "^3.10.2",
    "chokidar": "^3.5.2",
    "esbuild": "^0.13.15",
    "eslint": "^8.3.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "serve-handler": "^6.1.3"
  },
  "scripts": {
    "dev": "npm run develop",
    "develop": "node ./scripts/develop.js",
    "build": "node ./scripts/build.js",
    "lint": "eslint src",
    "test": "node test.js",
    "prepare": "husky install"
  }
